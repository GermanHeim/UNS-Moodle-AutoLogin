{
  "manifest_version": 2,
  "name": "UNS Moodle AutoLogin",
  "description": "Automáticamente inicia sesión en el Moodle de la UNS",
  "version": "1.0",
  "permissions": [
    "storage",
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["*://moodle.uns.edu.ar/moodle/*"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "icon/icon16.png",
      "48": "icon/icon48.png",
      "128": "icon/icon128.png"
    }
  }
}
